{
  "dashboard": {
    "id": null,
    "title": "MeatyMusic Security Performance Metrics",
    "tags": ["security", "performance", "meatymusic", "rls", "metrics"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Security Query Duration Distribution",
        "type": "histogram",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "sum(rate(meatymusic_security_query_duration_seconds_bucket[5m])) by (le)",
            "legendFormat": "{{le}}s",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "unit": "s"
          }
        },
        "options": {
          "bucketSize": 0.005
        }
      },
      {
        "id": 2,
        "title": "RLS Overhead by Operation Type",
        "type": "timeseries",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(meatymusic_rls_overhead_seconds_bucket[5m])) by (le, operation_type))",
            "legendFormat": "P95 {{operation_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(meatymusic_rls_overhead_seconds_bucket[5m])) by (le, operation_type))",
            "legendFormat": "P50 {{operation_type}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10
            },
            "unit": "s"
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["lastNotNull", "max", "mean"]
          }
        }
      },
      {
        "id": 3,
        "title": "Database Security Function Performance",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 9
        },
        "targets": [
          {
            "expr": "topk(10, avg_over_time(meatymusic_db_security_function_duration_seconds[5m]))",
            "legendFormat": "{{function_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.01
                },
                {
                  "color": "red",
                  "value": 0.1
                }
              ]
            },
            "unit": "s"
          }
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "function_name": "Security Function",
                "Value": "Avg Duration (s)"
              }
            }
          }
        ]
      },
      {
        "id": 4,
        "title": "Context Creation/Validation Performance",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 9
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(meatymusic_security_context_creation_duration_seconds_bucket[5m])))",
            "legendFormat": "P95 Context Creation",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(meatymusic_security_context_validation_duration_seconds_bucket[5m])))",
            "legendFormat": "P95 Context Validation",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.05
                },
                {
                  "color": "red",
                  "value": 0.2
                }
              ]
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 5,
        "title": "Memory Usage by Security Component",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 9
        },
        "targets": [
          {
            "expr": "sum by (component) (meatymusic_security_memory_usage_bytes)",
            "legendFormat": "{{component}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10,
              "stacking": {
                "mode": "normal",
                "group": "A"
              }
            },
            "unit": "bytes"
          }
        }
      },
      {
        "id": 6,
        "title": "Security Query Throughput",
        "type": "timeseries",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 17
        },
        "targets": [
          {
            "expr": "sum(rate(meatymusic_security_queries_total[5m]))",
            "legendFormat": "Total Queries/sec",
            "refId": "A"
          },
          {
            "expr": "sum by (query_type) (rate(meatymusic_security_queries_total[5m]))",
            "legendFormat": "{{query_type}}/sec",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10
            },
            "unit": "reqps"
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["lastNotNull", "max", "mean"]
          }
        }
      },
      {
        "id": 7,
        "title": "Database Connection Pool Impact",
        "type": "timeseries",
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 17
        },
        "targets": [
          {
            "expr": "meatymusic_db_pool_connections_active",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "meatymusic_db_pool_connections_idle",
            "legendFormat": "Idle Connections",
            "refId": "B"
          },
          {
            "expr": "rate(meatymusic_db_pool_connection_wait_time_seconds_sum[5m]) / rate(meatymusic_db_pool_connection_wait_time_seconds_count[5m])",
            "legendFormat": "Avg Wait Time",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Avg Wait Time"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "s"
                },
                {
                  "id": "custom.axisPlacement",
                  "value": "right"
                }
              ]
            }
          ]
        }
      },
      {
        "id": 8,
        "title": "Security Query Cache Performance",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 26
        },
        "targets": [
          {
            "expr": "sum(rate(meatymusic_security_cache_hits_total[5m])) / sum(rate(meatymusic_security_cache_requests_total[5m])) * 100",
            "legendFormat": "Cache Hit Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 9,
        "title": "RLS Policy Compilation Time",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 26
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(meatymusic_rls_policy_compilation_seconds_bucket[5m])) by (le))",
            "legendFormat": "P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(meatymusic_rls_policy_compilation_seconds_bucket[5m])) by (le))",
            "legendFormat": "P50",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 10,
        "title": "Resource Consumption Summary",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 26
        },
        "targets": [
          {
            "expr": "sum(rate(meatymusic_security_cpu_seconds_total[5m])) * 100",
            "legendFormat": "Security CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "sum(meatymusic_security_memory_usage_bytes) / 1024 / 1024",
            "legendFormat": "Security Memory (MB)",
            "refId": "B"
          },
          {
            "expr": "sum(rate(meatymusic_security_disk_io_bytes_total[5m]))",
            "legendFormat": "Security Disk I/O (B/s)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            }
          }
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "Metric": "Resource",
                "Value": "Current Usage"
              }
            }
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": true,
          "multi": true,
          "name": "instance",
          "options": [],
          "query": "label_values(meatymusic_security_query_duration_seconds_bucket, instance)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        },
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "hide": 0,
          "includeAll": true,
          "multi": true,
          "name": "operation_type",
          "options": [],
          "query": "label_values(meatymusic_rls_overhead_seconds_bucket, operation_type)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        },
        {
          "current": {
            "selected": false,
            "text": "5m",
            "value": "5m"
          },
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "name": "rate_interval",
          "options": [
            {
              "selected": false,
              "text": "1m",
              "value": "1m"
            },
            {
              "selected": true,
              "text": "5m",
              "value": "5m"
            },
            {
              "selected": false,
              "text": "15m",
              "value": "15m"
            },
            {
              "selected": false,
              "text": "30m",
              "value": "30m"
            }
          ],
          "query": "1m,5m,15m,30m",
          "skipUrlSync": false,
          "type": "custom"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": {
            "type": "datasource",
            "uid": "grafana"
          },
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "enable": true,
          "expr": "ALERTS{alertname=~\".*Performance.*\", alertstate=\"firing\"}",
          "iconColor": "orange",
          "name": "Performance Alerts",
          "titleFormat": "{{alertname}}: {{summary}}"
        }
      ]
    },
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [
      {
        "asDropdown": false,
        "icon": "external link",
        "includeVars": true,
        "keepTime": true,
        "tags": ["security"],
        "targetBlank": true,
        "title": "Security Overview",
        "type": "dashboards"
      }
    ],
    "liveNow": false,
    "panels": [],
    "schemaVersion": 37,
    "version": 0,
    "weekStart": ""
  }
}
